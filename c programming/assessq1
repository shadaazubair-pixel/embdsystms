#include<stdio.h>



int main(){
    int arr[x];

    printf("enter the size of array:");
    scanf("%d",&x);

    for(int i=0;i<x;i++){
        printf("enter value of arr[i]:\n");
        scanf("%d",&arr[x]);
        
    }
    for(int i=0;i<x;i++){
        printf("the elements of array are:");
        printf("%d\t",arr[i]);
    }
    return 0;

}