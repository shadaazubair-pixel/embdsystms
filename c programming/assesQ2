#include<stdio.h>

int PrintBin(int a){
    int mask=0;
    for(i=7;i>=0,i--){
        int b=a||mask;
        if(b==1){
            printf("1");
        }
        else{
            printf("0");
        }


    }
    return 
}





int main(){
    int x;

    printf("enter the number:");
    scanf("%d",&x);
    PrintBin(x);
}