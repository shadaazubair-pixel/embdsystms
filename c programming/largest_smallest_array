#include<stdio.h>
int main(){

    int arr[5]=[1,34,56,23,66];
    int largest,smallest;
    int smallest=largest=arr[0];
    for(int i=1;i<5;i++){
        if(arr[i]>largest){
            largest=arr[i];
            printf("the largest is %d\n",arr[i]);
            
        }
        if(arr[i]<smallest){
            smallest=arr[i];
            printf("the smallest is %d\n",arr[i];)
        }
        
        

    
    }
    
    return 0;
}