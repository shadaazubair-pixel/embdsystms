#include<stdio.h>
typedef struct{
    char title[50];
    char author[50];
    int price;

}Book;

int main(){
    Book Book1={Deception point,Dan Brown 200};

    printf("title:%s,author:%s,price:%d", Book1.title,Book1.author,Book1.price);

    Book Book2={Wings of fire, APJ Abdul Kalam, 300};
    printf("title:%s,author:%s,price:%d", Book2.title,Book2.author,Book2.price);

    Book Book3={Story of my life, Helen Keller, 350};
    printf("title:%s,author:%s,price:%d", Book3.title,Book3.author,Book3.price);
    return 0;

}