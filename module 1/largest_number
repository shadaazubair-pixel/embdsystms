#include<stdio.h>
int main(){

    int num1,num2,num3,largest;

    printf("enter the 1st number:");
    scanf("%d",&num1);

    printf("enter the 2nd number:");
    scanf("%d",&num2);

    printf("enter the 3rd number:");
    scanf("%d",&num3);

    largest=num1;

    if(num2<largest&&num3<largest){
        printf("num1 is the largest");
    }

    else if(num2>largest&&num2>num3){
        printf("num2 is largest number");
    }

    else{
        printf("num3 is the largest");
    }
    return 0;

}
