#include<stdio.h>

int main(){

    int num;
    printf("enter the number:\n");
    scanf("%d",&num);

    if(num>0){
        printf("the number is positive\n");
    }
    else if(num<=0){
        printf("the number is negetive\n");

    }
    else{
        printf("the number is 0");
    }
    return 0;
}